import java.util.*;

public class Main {

    public static void main(String[] args) {
        Scanner scn = new Scanner(System.in);
    
        int n = scn.nextInt();
        
        for(int count = 1; count <= 10; count++){
            
            System.out.println(n + " * " + count + " = " + n*count);
        }
    }
}
