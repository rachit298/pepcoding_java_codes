import java.util.*;

public class Main {

    public static void main(String[] args) {
        Scanner scn = new Scanner(System.in);

        int n = scn.nextInt();
        int count = 1;
        
        for(int row = 1; row <= n; row++){
            
            for(int col = 1; col <= row; col++, count++){
                
                System.out.print(count + "\t");
            }
            
            System.out.println();
        }
    }
}
